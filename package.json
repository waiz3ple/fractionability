{
    "name": "fractionability",
    "version": "2.1.8",
    "description": "A lightweight JavaScript library for converting decimals and strings into accessible, screen-reader-friendly fractions. Generate MathML for enhanced accessibility and ensure inclusive fraction display on the web.",
    "license": "MIT",
    "author": "Wasiu Ramoni",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "http://github.com/waiz3ple/fractionability"
    },
    "scripts": {
        "tsc": "tsc",
        "test": "jest",
        "build": "rollup -c",
        "preview": "vite preview",
        "prepare": "npm run build"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^28.0.2",
        "@rollup/plugin-multi-entry": "^6.0.1",
        "@rollup/plugin-node-resolve": "^16.0.0",
        "@rollup/plugin-typescript": "^12.1.2",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.13.1",
        "@vitejs/plugin-react": "^4.2.1",
        "esm": "^3.2.25",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "rollup": "^4.34.6",
        "rollup-plugin-preserve-shebangs": "^0.2.0",
        "rollup-plugin-typescript2": "^0.36.0",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.3",
        "vite": "^6.0.11"
    },
    "keywords": [
        "fraction",
        "fraction-converter",
        "fraction-library",
        "accessibility",
        "mathml",
        "screen-reader-friendly",
        "decimal-to-fraction",
        "fraction-display",
        "mathjs",
        "web-accessibility",
        "fraction-formatting",
        "human-readable-fractions",
        "mixed-numbers",
        "improper-fractions",
        "javascript-library",
        "npm-package",
        "inclusive-web",
        "fraction-mathml",
        "fraction-api",
        "fraction-tool"
    ],
    "engines": {
        "node": ">=14.0.0"
    },
    "browserslist": [
        "> 0.5%",
        "last 2 versions",
        "not dead"
    ],
    "overrides": {
        "ajv": "^8.17.1",
        "whatwg-url": "^14.0.0"
    }
}