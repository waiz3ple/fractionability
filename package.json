{
    "name": "fractionability",
    "version": "2.2.3",
    "description": "A lightweight JavaScript/TypeScript library for accessible fraction conversion, arithmetic, and MathML display.",
    "license": "MIT",
    "author": "Wasiu Ramoni <waiz3ple@gmail.com>",
    "homepage": "https://github.com/waiz3ple/fractionability#readme",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.cjs"
            }
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/waiz3ple/fractionability.git"
    },
    "bugs": {
        "url": "https://github.com/waiz3ple/fractionability/issues"
    },
    "scripts": {
        "build": "rollup -c",
        "test": "jest",
        "test:watch": "jest --watch",
        "typecheck": "tsc --noEmit",
        "prepare": "npm run build",
        "prepublishOnly": "npm run test && npm run typecheck && npm run build",
        "preview": "vite preview"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^28.0.2",
        "@rollup/plugin-node-resolve": "^16.0.0",
        "@rollup/plugin-typescript": "^12.1.2",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.13.1",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "rollup": "^4.24.0",
        "rollup-plugin-dts": "^6.1.1",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "tslib": "^2.8.1",
        "typescript": "^5.7.3",
        "vite": "^6.0.11"
    },
    "keywords": [
        "fraction",
        "fractions",
        "math",
        "accessibility",
        "mathml",
        "decimal",
        "ratio",
        "mixed-number",
        "screen-reader",
        "javascript",
        "typescript",
        "fraction-converter",
        "web-a11y",
        "web-accessibility",
        "fractionability"
    ],
    "engines": {
        "node": ">=18.0.0"
    },
    "browserslist": [
        "> 0.5%",
        "last 2 versions",
        "not dead",
        "not ie <= 11"
    ],
    "packageManager": "npm@10.8.3"
}